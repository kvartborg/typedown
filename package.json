{
  "name": "markdown",
  "description": "",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "test": "npm run lint && jest --coverage",
    "test:watch": "jest --watch",
    "lint": "standard | snazzy; exit 0",
    "lint:fix": "standard --fix | snazzy; exit 0",
    "build": "babel src/ -D -d ./lib; exit 0",
    "build:watch": "babel src/ -D -d ./lib --watch",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build && npm test"
  },
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "module"
  ],
  "author": {
    "name": "Frederik Kvartborg Albertsen",
    "email": "hello@kvartborg.net",
    "url": "https://kvartborg.net"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": "/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "babel-jest": "^21.0.2",
    "coveralls": "^3.0.2",
    "jest": "^21.1.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
  },
  "jest": {
    "modulePaths": [
      "src"
    ]
  },
  "dependencies": {
    "mathjax": "^2.7.5",
    "mathjax-node": "^2.1.1",
    "node-watch": "^0.5.8"
  }
