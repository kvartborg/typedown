{
  "name": "typedown",
  "description": "",
  "version": "0.0.4",
  "license": "MIT",
  "scripts": {
    "test": "# npm run lint && jest --coverage",
    "test:watch": "jest --watch",
    "lint": "standard | snazzy; exit 0",
    "lint:fix": "standard --fix | snazzy; exit 0",
    "build": "babel src/ -D -d ./lib; exit 0",
    "build:watch": "babel src/ -D -d ./lib --watch",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build && npm test"
  },
  "main": "lib/typedown.js",
  "module": "src/typedown.js",
  "bin": {
    "typedown": "./bin/cli.js",
    "td": "./bin/cli.js"
  },
  "files": [
    "bin",
    "lib",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "module"
  ],
  "author": {
    "name": "Frederik Kvartborg Albertsen",
    "email": "hello@kvartborg.net",
    "url": "https://kvartborg.net"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": "/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "babel-jest": "^21.0.2",
    "coveralls": "^3.0.2",
    "jest": "^21.1.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
  },
  "jest": {
    "modulePaths": [
      "src"
    ]
  },
  "dependencies": {
    "csv-parse": "^4.3.1",
    "d3": "^5.8.0",
    "d3-node": "^2.1.0",
    "mathjax": "^2.7.5",
    "mathjax-node": "^2.1.1",
    "meow": "^5.0.0",
    "node-fetch": "^2.3.0",
    "node-watch": "^0.5.8",
    "remark-html": "^9.0.0",
    "remark-parse": "^6.0.3",
    "remark-stringify": "^6.0.4",
    "unified": "^7.1.0",
    "unified-stream": "^1.0.4"
  }
